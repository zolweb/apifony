{% autoescape false %}
<?php

namespace {{ file.namespace }};

interface {{ file.className }}
{
{%~ for action in file.actions %}
    {%~ for method in action.methods %}
    public function {{ method.name }}(
        {%~ for param in method.parameters %}
        {{ param.schema.type.methodParameterType }} ${{ param.variableName }},
        {%~ endfor %}
        {%~ if requestBodyPayloadTypeNormalizedName is not same as ('Empty') %}
        {{ requestBodyPayloadType.methodParameterType }} $requestBodyPayload,
        {%~ endif %}
    ):
        {%~ for response in operation.allPossibleResponsesForContentType(responseContentType) %}
        {{ response }}{%~ if not loop.last %} |
        {%~ else %};
        {%~ endif %}
        {%~ endfor %}
    {%~ endfor %}
    {%~ if not loop.last %}

    {%~ endif %}
{%~ endfor %}
}
{% endautoescape %}