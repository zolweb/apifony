{% autoescape false %}
<?php

namespace {{ handler.namespace }};

interface {{ handler.className }}
{
{%~ for action in handler.actions %}
    {%~ for case in action.cases %}
    public function {{ case.name }}(
        {%~ for param in case.parameters %}
        {{ param.phpType }} ${{ param.variableName }},
        {%~ endfor %}
        {%~ if case.requestBodyPayloadParameter %}
        {{ case.requestBodyPayloadParameterPhpType }} $requestBodyPayload,
        {%~ endif %}
    ):
        {%~ for response in case.responses %}
        {{ response.className }}{%~ if not loop.last %} |
        {%~ else %};
        {%~ endif %}
        {%~ endfor %}
        {%~ if not loop.last %}

        {%~ endif %}
    {%~ endfor %}
    {%~ if not loop.last %}

    {%~ endif %}
{%~ endfor %}
}
{% endautoescape %}