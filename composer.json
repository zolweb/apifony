{
    "name": "zolteam/ogen",
    "license": "AGPL-3.0-or-later",
    "bin": [
        "ogen"
    ],
    "require": {
        "php": ">=8.1",
        "nikic/php-parser": "^4.16 || ^5.0",
        "phpstan/phpdoc-parser": "^1.21",
        "symfony/string": "^6.3 || ^7.0",
        "symfony/uid": "^6.3 || ^7.0",
        "symfony/yaml": "^6.3 || ^7.0",
        "symfony/console": "^6.3 || ^7.0"
    },
    "require-dev": {
        "egulias/email-validator": "^2.1.10",
        "friendsofphp/php-cs-fixer": "3.62.0",
        "phpstan/extension-installer": "1.4.1",
        "phpstan/phpstan": "1.11.10",
        "phpstan/phpstan-doctrine": "1.5.0",
        "phpstan/phpstan-phpunit": "1.4.0",
        "phpstan/phpstan-symfony": "1.4.6",
        "phpunit/phpunit": "9.5.5",
        "symfony/filesystem": "^6.3 || ^7.0",
        "symfony/framework-bundle": "^6.3 || ^7.0",
        "symfony/phpunit-bridge": "^6.3 || ^7.0",
        "symfony/test-pack": "^1.1",
        "zol/test-openapi-server": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Zol\\Ogen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zol\\Ogen\\Tests\\": "tests/",
            "Zol\\Ogen\\Tests\\TestOpenApiServer\\" : "tests/bundle/src"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "tests/bundle"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
