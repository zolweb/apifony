{
    "name": "zolteam/ogen",
    "license": "AGPL-3.0-or-later",
    "bin": [
        "ogen"
    ],
    "require": {
        "symfony/string": "^6.0 || ^7.0",
        "symfony/yaml": "^6.0 || ^7.0",
        "twig/twig": "^3.6"
    },
    "require-dev": {
        "symfony/test-pack": "^1.1",
        "symfony/filesystem": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "zol/test-openapi-server": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Zol\\Ogen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zol\\Ogen\\Tests\\": "tests/",
            "Zol\\Ogen\\Tests\\TestOpenApiServer\\" : "tests/bundle/src"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "tests/bundle"
        }
    ]
}
